for (i = 1; i <= 500; i++) {
    var materias = ["Matemáticas", "Física", "Química", "Historia", "Lengua", "Inglés", "Biología", "Geografía", "Filosofía", "Arte"];
    for (j = 0; j < materias.length; j++) {
        db.prubaUCA.insert({
            registro: i,
            materias: materias[j],
            calificacion: Math.floor(Math.random() * 10) + 1
        });
    }
}


db.prubaUCA.find().pretty()
